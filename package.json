    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --content-base www/ --open --inline --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "browser": "cordova prepare browser && cross-env NODE_ENV=development webpack-dev-server --content-base www/ --open --inline --hot",
        "check": "cordova requirements",
        "ios": "cordova prepare ios && cordova run ios && cross-env NODE_ENV=development webpack-dev-server --content-base www/ --open --inline --hot",
        "lint": "eslint --ext .js,.vue src",
        "postinstall": "npm run build && cordova prepare",
        "dist": "gulp dist"
    },
